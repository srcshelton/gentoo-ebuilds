+
+&mipi2_csi2 {
+        status = "okay";
+
+#ifdef FF_COMPATIBLE
+		firefly-compatible;
+#endif
+        ports {
+                #address-cells = <1>;
+                #size-cells = <0>;
+
+                port@0 {
+                        reg = <0>;
+                        #address-cells = <1>;
+                        #size-cells = <0>;
+
+                        mipi2_csi2_input: endpoint@1 {
+                                reg = <1>;
+                                remote-endpoint = <&csidphy0_out>;
