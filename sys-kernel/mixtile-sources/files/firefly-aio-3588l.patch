+
+&i2c3 {
+        status = "okay";
+        pinctrl-names = "default";
+	pinctrl-0 = <&i2c3m0_xfer>;
+
+ 	XC7160: XC7160b@1b{
+               compatible = "firefly,xc7160";
+               status = "okay";
